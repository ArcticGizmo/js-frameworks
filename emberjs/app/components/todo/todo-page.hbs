<div class='todo-page'>
  <div class='add'>
    <button {{on 'click' this.onAdd}}>Create New</button>
  </div>
  <div class='cards'>
    {{#each this.store._todos as |todo index|}}
      {{!-- <Todo::TodoCard
        class='example'
        @title={{todo.title}}
        @completed={{todo.completed}}
        @onToggle={{this.onToggle}}
        @onTextChange={{fn this.onTextChange 1 2 3}}
        {{action 'a'}}
        @onDelete={{this.onDelete}}
        apple=7
        @egg=9
      /> --}}

      {{!-- <Todo::TodoCard
        @title={{todo.title}}
        @completed={{todo.completed}}
        @onToggle='{{this.a}}'
        {{on 'bacon' this.b}}
        {{action 'eggplant'}}
      /> --}}
    {{/each}}
  </div>
</div>