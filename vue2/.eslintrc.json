{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "plugin:vue/essential",
  "parserOptions": {
    "exmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error",
    "eqeqeq": ["error", "smart"],
    "vue/no-unused-components": "warn",
    "vue/multi-word-component-names": "off"
  },
  "globals": {
    "module": "readonly",
    "require": "readonly"
  }
}
